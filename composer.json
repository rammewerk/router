{
  "name": "rammewerk/router",
  "type": "library",
  "description": "Simple router for PHP",
  "homepage": "https://rammewerk.com",
  "license": "MIT",
  "keywords": [
    "router"
  ],
  "authors": [
    {
      "name": "Kristoffer Follestad",
      "email": "kristoffer@rammewerk.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rammewerk\\Router\\": "src/"
    },
    "exclude-from-classmap": [
      "tests/",
      ".idea/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rammewerk\\Router\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "rammewerk/container": "^1.2",
    "nyholm/psr7": "^1.8"
  }
}